{
  "name": "rgorai-npm-packages",
  "homepage": "https://rgorai.github.io/npm-packages/",
  "description": "Ron Gorai's React + TS boilerplate for developing and demoing npm packages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install && chmod ug+x .husky/*",
    "pre-commit": "lint-staged --quiet",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "@rgorai/eslint-config/.eslintrc"
  },
  "prettier": "@rgorai/eslint-config/.prettierrc.js",
  "stylelint": {
    "extends": "@rgorai/eslint-config/.stylelintrc"
  },
  "lint-staged": {
    "*": "yarn lint"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.0",
    "@rgorai/eslint-config": "^1.0.19",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@uiw/react-markdown-preview": "^4.1.13",
    "bootstrap": "^5.2.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-json-pretty": "^2.2.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.0.4",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/react-router-bootstrap": "^0.26.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "stylelint": "^15.6.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-prettier": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
